# Variables de entorno para CCB Talento Latam
# Copia este archivo como .env y configura tus valores reales

# Token de autorización para la API de Hilos (REQUERIDO)
HILOS_API_TOKEN=hilos_token

# Token de acceso para el frontend (REQUERIDO)
FRONTEND_ACCESS_TOKEN=token

# ID del flujo específico (OPCIONAL - tiene valor por defecto)
HILOS_FLOW_ID=0684111b-3948-7ce2-8000-b20bbb1bd564

# Configuración de Flask (OPCIONAL)
FLASK_ENV=production
FLASK_DEBUG=False

# Puerto para desarrollo local (OPCIONAL)
PORT=8080
